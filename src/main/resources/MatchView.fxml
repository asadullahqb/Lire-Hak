<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>

<AnchorPane
        prefHeight="600.0"
        prefWidth="1000.0"
        xmlns="http://javafx.com/javafx/11.0.1"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="EngineControllers.MatchController">
   <children>
      <Label
              layoutX="25.0"
              layoutY="14.0"
              prefHeight="26.0"
              prefWidth="200.0"
              text="Calculate Image Similarity">
         <font>
            <Font
                    size="17.0" />
         </font></Label>
      <TextField
              fx:id="imgPath1"
              focusTraversable="false"
              promptText="No image selected."
              editable="false"
              layoutX="25.0"
              layoutY="60.0"
              prefHeight="25.0"
              prefWidth="439.0" />
      <Button
              fx:id="chooseImgBtn1"
              layoutX="25.0"
              layoutY="97.5"
              mnemonicParsing="false"
              onAction="#btnOneHandler"
              prefHeight="25.0"
              prefWidth="159.0"
              text="Choose Image" />
      <TextField
              fx:id="imgPath2"
              focusTraversable="false"
              promptText="No image selected."
              editable="false"
              layoutX="480.0"
              layoutY="60.0"
              prefHeight="25.0"
              prefWidth="439.0" />
      <Button
              fx:id="chooseImgBtn2"
              layoutX="480.0"
              layoutY="97.5"
              mnemonicParsing="false"
              onAction="#btnTwoHandler"
              prefHeight="25.0"
              prefWidth="159.0"
              text="Choose Image" />
      <Button
              fx:id="startCalcBtn"
              layoutX="25.0"
              layoutY="462.0"
              mnemonicParsing="false"
              onAction="#startCalc"
              prefHeight="25.0"
              prefWidth="240.0"
              text="Calculate Similarity" />
      <ComboBox
              fx:id="FeatureSelector"
              layoutX="274.0"
              layoutY="462.0">
         <items>
            <FXCollections
                    fx:factory="observableArrayList">
               <String
                       fx:value="CEDD"/>
               <String
                       fx:value="FCTH"/>
               <String
                       fx:value="AutoColorCorrelogram"/>
            </FXCollections>
         </items>
      </ComboBox>
      <Label
              layoutX="25.0"
              layoutY="499.0"
              text="Results:" />
      <TextArea
              fx:id="Output"
              layoutX="25.0"
              layoutY="522.0"
              prefHeight="57.0"
              prefWidth="894.0" />
      <AnchorPane
              layoutX="25.0"
              layoutY="135.0"
              prefHeight="315.0"
              prefWidth="439.0"
              style="-fx-background-color: #555555;">
         <children>
            <Label
                    fx:id="temp"
                    layoutX="160.17"
                    layoutY="145.0"
                    text="Drag image here"
                    textFill="WHITE">
               <font>
                  <Font
                          size="16.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane
              layoutX="480.0"
              layoutY="135.0"
              prefHeight="315.0"
              prefWidth="439.0"
              style="-fx-background-color: #555555;">
         <children>
            <Label
                    layoutX="160.17"
                    layoutY="147.0"
                    text="Drag image here"
                    textFill="WHITE">
               <font>
                  <Font
                          size="16.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <ImageView
              fx:id="firstImage"
              fitHeight="315.0"
              fitWidth="439.0"
              layoutX="25.0"
              layoutY="134.0"
              onDragDropped="#image1DropHandler"
              onDragOver="#imageDragOver"
              pickOnBounds="true"
              preserveRatio="true">
         <cursor>
            <Cursor
                    fx:constant="MOVE" />
         </cursor>
      </ImageView>
      <ImageView
              fx:id="secondImage"
              fitHeight="315.0"
              fitWidth="439.0"
              layoutX="480.0"
              layoutY="134.0"
              onDragDropped="#image2DropHandler"
              onDragOver="#imageDragOver"
              pickOnBounds="true"
              preserveRatio="true">
         <cursor>
            <Cursor
                    fx:constant="MOVE" />
         </cursor>
      </ImageView>
   </children>
</AnchorPane>